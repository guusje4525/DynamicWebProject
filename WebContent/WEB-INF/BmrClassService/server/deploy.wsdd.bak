<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from BmrClassService WSDL service -->

  <service name="BmrClass" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://server"/>
      <parameter name="wsdlServiceElement" value="BmrClassService"/>
      <parameter name="schemaQualified" value="http://server"/>
      <parameter name="wsdlServicePort" value="BmrClass"/>
      <parameter name="className" value="server.BmrClassSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="BmrClass"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="generateBmr" qname="operNS:generateBmr" xmlns:operNS="http://server" returnQName="retNS:generateBmrReturn" xmlns:retNS="http://server" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:weight" xmlns:pns="http://server" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:height" xmlns:pns="http://server" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:gender" xmlns:pns="http://server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:age" xmlns:pns="http://server" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="generateBmr"/>

  </service>
</deployment>
